{
  "$schema": "./node.schema.json",
  "displayName": "Foo",
  "name": "foo",
  "iconUrl": "https://raw.githubusercontent.com/yehorkardash/n8n-nodes/refs/heads/main/icon.svg",
  "group": ["transform"],
  "version": 1,
  "subtitle": "={{$parameter[\"operation\"] + \": \" + $parameter[\"resource\"]}}",
  "description": "Use the Foo API",
  "defaults": {
    "name": "Foo"
  },
  "usableAsTool": true,
  "inputs": ["main"],
  "outputs": ["main"],
  "credentials": [
    {
      "name": "fooApi",
      "required": true
    }
  ],
  "requestDefaults": {
    "returnFullResponse": true,
    "baseURL": "={{$credentials.url.replace(new RegExp(\"/$\"), \"\")}}",
    "headers": {}
  },
  "properties": [
    {
      "displayName": "Resource",
      "name": "resource",
      "type": "options",
      "noDataExpression": true,
      "options": [
        {
          "name": "Foo",
          "value": "foo"
        }
      ],
      "default": "foo"
    }
  ]
}
